<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<h1>Laundry</h1>
<body>
    <table>
        <%laundries.forEach(function(laundry){%>
            <tr>
                <td><%= laundry.name %></td>
                <td><%= laundry.address %></td>
                <td><%= laundry.pricePerKg %></td>
                <td><%= laundry.photo %></td>
                <td><%= laundry.city %></td>
                <td><a href="#">order</a></td>
            </tr>
        <%})%>
    </table>
</body>
</html>